#ifndef POINT_H_
#define POINT_H_

#include <iostream>
using namespace std;

class Point{
public:
     Point(double x = 0.5, double y = 0.6);
     void SetCoordinate(double, double);
     double getX();
     double getY();
     friend ostream & operator << (ostream &, const Point&);

protected:
     double x, y;
};



#endif /* POINT_H_ */
