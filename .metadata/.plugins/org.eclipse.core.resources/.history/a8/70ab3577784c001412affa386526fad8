#ifndef POINT_H_
#define POINT_H_

class Point{
public:
     Point(double, double);
     void SetCoordinate(double, double);
     double getX() {return x;}
     double getY() {return y;}

protected:
     double x, y;
};



#endif /* POINT_H_ */
