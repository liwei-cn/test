#ifndef POINT_H_
#define POINT_H_

#include <iostream>]
using namespace std;

class Point{
public:
     Point(double, double);
     void SetCoordinate(double, double);
     double getX() {return x;}
     double getY() {return y;}
     friend ostream & operator << (ostream &, const Point&);

protected:
     double x, y;
};



#endif /* POINT_H_ */
