#include "cylinder.h"

Cylinder::Cylinder(double x, double y, double r, double h): Point(x, y), Circle(x,y,r),height(h)
{

}

Cylinder::~Cylinder()
{
	cout << "cylinder destroyed!" << endl;
}

void Cylinder::SetHeight (double h)
{
	height = h;
}

