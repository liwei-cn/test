#include <iostream>
#include "point.h"
#include "circle.h"
#include "cylinder.h"
using namespace std;


int main(int agrc, char* agrv[])
{

	Cylinder cy(0.1,0.2,0.3,0.4);
    cout << cy;

	Circle c(0.1,0.2,0.3);
    cout << c;

    Circle *p;
    p = &c;
    cout << p->area();

    return 0;
}



